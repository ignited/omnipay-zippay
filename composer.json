{
    "name": "ignited/omnipay-zippay",
    "description": "Provides ZipPay support for Omnipay payment processing library",
    "keywords": [
        "omnipay",
        "ZipPay",
        "zip"
    ],
    "homepage": "https://github.com/ignited/omnipay-zippay",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Condie",
            "email": "daniel@ignitedlabs.com.au"
        }
    ],
    "require": {
        "omnipay/common": "~3.0"
    },
    "require-dev": {
        "omnipay/tests": "~3.0",
        "squizlabs/php_codesniffer": "^3",
        "phpro/grumphp": "^0.14"
    },
    "autoload": {
        "psr-4": {
            "Omnipay\\ZipPay\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Omnipay\\ZipPay\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 src/",
        "fix-style": "phpcbf -p --standard=PSR2 src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
